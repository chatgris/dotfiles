[color]
    branch = auto
    diff = auto
    interactive = auto
    status = auto
[alias]
    br = branch
    bra = branch -a
    ch = cherry-pick
    ci = commit
    co = checkout
    df = diff
    fe = fetch -p
    i = remote -v
    l = log
    la = log -p -1 HEAD
    lo = log -p
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    sm = submodule update
    st = status
[push]
    default = current
